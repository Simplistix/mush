[run]
branch = True
parallel = True

[report]
exclude_lines = 
  # the original exclude
  pragma: no cover

  # code executed only when tests fail
  'No exception raised!'
  self\.fail\('Expected

  # example code that we don't want to cover with pragma statements
  guppy =

[paths]
src = 
  ./mush/
  C:\Jenkins\workspace\mush-buildout\*\mush
  /var/lib/jenkins/slave/workspace/mush-buildout/*/mush
  /Users/Shared/Jenkins/Home/workspace/mush-buildout/*/mush
  ./tests/
  C:\Jenkins\workspace\tests-buildout\*\tests
  /var/lib/jenkins/slave/workspace/tests-buildout/*/tests
  /Users/Shared/Jenkins/Home/workspace/tests-buildout/*/tests
